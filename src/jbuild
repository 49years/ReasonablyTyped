(jbuild_version 1)

(executable
  ((name cli)
    (modules (cli))
    (public_name retyped)
    (libraries (retyped))))

(executable
  ((name retyped_node)
    (modules (retyped_node))
    (js_of_ocaml ((flags (--pretty))))
    (libraries (retyped js_of_ocaml))))

(executable
  ((name test)
    (modules (test))
    (libraries (retyped_test spec))))

(alias
 ((name runtest)
  (deps (test.exe))
  (action (run ${<}))))
